<header id="header" class="no-select">
	
	<h1>
		<a href="index">Triangelos</a>
		<a href="javascript:void(0)" id="menu-toggle">{{#svg}}menu{{/svg}}</a>
	</h1>
	
	<ul id="main-menu">
		<li><a href="index">{{#svg}}home{{/svg}}<span>{{#_}}title/Home{{/_}}</span></a>
		<li><a href="music">{{#svg}}cart{{/svg}} {{#_}}title/Music{{/_}}</a>
		<li><a href="video">{{#svg}}youtube-symbol{{/svg}} {{#_}}title/Video{{/_}}</a>
		<li><a href="calendar">{{#_}}title/Calendar{{/_}}</a>
		<li><a href="choir">{{#_}}title/Choir{{/_}}</a>
		<li><a href="members">{{#_}}title/Members{{/_}}</a>
		<li><a href="contact">{{#_}}title/Contact{{/_}}</a>
		<li><a href="member-area">{{#svg}}tools{{/svg}}<span>{{#_}}title/Member area{{/_}}</span></a>
	</ul>
	<ul id="language-menu">
		{{#_.languages}}
		<li><a class="language" href="{{href}}/{{this}}" rel="alternate" hreflang="{{lang}}" lang="{{lang}}" title="{{name}}" data-host="{{host}}"><img src="_/flag/{{flag}}.png"></a>
		{{/_.languages}}
	</ul>
	<div id="bg-1"></div>

</header>
